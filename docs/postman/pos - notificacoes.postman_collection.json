							"    method: 'POST',\r",
							"    header: 'Content-Type: application/json',\r",
							"    body: {\r",
							"        mode: 'raw',\r",
							"        raw: JSON.stringify({ clientId: 'demo-client', password: 'demo123' })\r",
							"    }\r",
							"}, function (err, response) {\r",
							"    if (err) {\r",
							"        console.error(err);\r",
							"    } else {\r",
							"        if (response.code === 200) {\r",
							"            var responseBody = response.json();\r",
							"            pm.collectionVariables.set('access_token', responseBody.token);\r",
							"        } else {\r",
							"            console.error('Failed to login. Response code:', response.code);\r",
							"        }\r",
							"    }\r",
							"});"
